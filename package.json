{
  "name": "es6-app",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-lib": "webpack --config webpack.config.lib.js && cp dist/lib/*.min.js demo/src/",
    "start:browser": "npm run build-lib && webpack-dev-server --config webpack.dev.js --open",
    "start:node": "nodemon --exec babel-node demo/src/index.js",
    "test": "npm run build-lib && cross-env NODE_ENV=test nyc mocha --require @babel/register --colors ./test/*.spec.js",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "publish": "npm run build && cd dist/lib && npm publish --access public"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^3.1.1",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "csv-loader": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "docdash": "^1.1.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mocha": "^10.2.0",
    "nodemon": "^1.18.10",
    "nyc": "^14.0.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-strip-log-loader": "^0.1.5",
    "xml-loader": "^1.2.1"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "d2": "^31.4.0",
    "lodash": "^4.17.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
